<?xml version="1.0" encoding="UTF-8"?>
<container:container xml:base="http://www.escidoc.de/"
	xmlns:container="http://www.escidoc.de/schemas/container/0.8"
	xmlns:prop="http://escidoc.de/core/01/properties/" 
	xmlns:srel="http://escidoc.de/core/01/structural-relations/"
	xmlns:metadata-records="http://www.escidoc.de/schemas/metadatarecords/0.5"
	xmlns:xlink="http://www.w3.org/1999/xlink">

	<container:properties>
		<srel:context xlink:href="/ir/context/###CONTEXT_ID###" />
		<srel:content-model xlink:href="/cmm/content-model/###CONTENT_MODEL_ID###" />
		<prop:content-model-specific />
	</container:properties>

	<metadata-records:md-records>
		<metadata-records:md-record name="escidoc">
			<metadata xmlns:dc="http://purl.org/dc/elements/1.1/">
				<dc:title>Container Title</dc:title>
				<dc:description>A minimal container used in examples.</dc:description>
			</metadata>
		</metadata-records:md-record>
	</metadata-records:md-records>

</container:container>
